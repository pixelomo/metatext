{
    "build": {
      "env": {
        "PIP_NO_CACHE_DIR": "off",
        "PIP_DISABLE_PIP_VERSION_CHECK": "on"
      },
      "install": {
        "use": "vercel-build"
      },
      "vercel-build": {
        "packages": [
          "libxml2-dev",
          "libxslt1-dev"
        ]
      }
    }
}
